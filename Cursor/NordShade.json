  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#57637D"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.template", "constant.other.symbol"],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "string.constant"],
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.word",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#CACFD6"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Storage type",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Variable start",
      "scope": ["punctuation.definition.variable"],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#88C0D0"
      }
    }
  ]
}
